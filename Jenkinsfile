pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Building...'
                // Chạy lệnh build của ứng dụng
            }
        }
        stage('Test') {
            steps {
                echo 'Testing...'
                // Chạy lệnh kiểm thử
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying...'
                // Thực hiện các bước deploy
            }
        }
    }
}

